    </joint>

    <!-- Plugin to control differential locomotion-->

   <plugin filename="libgazebo_ros_skid_steer_drive.so" name="skid_steer_drive_controller">
      <updateRate>25.0</updateRate>
      <robotNamespace>p3at/</robotNamespace>
      <leftFrontJoint>p3at/front_left_wheel_joint</leftFrontJoint>
      <rightFrontJoint>p3at/front_right_wheel_joint</rightFrontJoint>
      <leftRearJoint>p3at/back_left_wheel_joint</leftRearJoint>
      <rightRearJoint>p3at/back_right_wheel_joint</rightRearJoint>
      <wheelSeparation>0.4</wheelSeparation>
      <wheelDiameter>0.215</wheelDiameter>
      <robotBaseFrame>/p3at/base_link</robotBaseFrame>
      <MaxForce>5.0</MaxForce>
      <torque>200</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <broadcastTF>1</broadcastTF>
      <covariance_x>0.0001</covariance_x>
      <covariance_y>0.0001</covariance_y>
      <covariance_z>0.0001</covariance_z>
      <covariance_yaw>0.0100</covariance_yaw>
    </plugin>

    <plugin name='plugin_tf_p3at_gazebo_ros' filename='libplugin_tf_p3at_gazebo_ros.so'>
      <robotNamespace>p3at</robotNamespace>
      <updateRate>50.0</updateRate>
    </plugin>

    

