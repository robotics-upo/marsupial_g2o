<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="tether_marsupial">
    <self_collide>true</self_collide>
    <include>
      <uri>model://pionner3at</uri>
      <pose>0 0 0 0 0 0</pose>
    </include> 

    <include>
      <uri>model://tether</uri> 
      <pose>0.26 0 0.287 0 0 0</pose>
    </include>

    <include>
      <uri>model://ardrone</uri>
      <pose>2.24 0 0.327 0 0 0</pose>
    </include> 


    <joint name="marsupial/pionner_tether_joint" type="revolute">
      <pose>0 0 -0.01 0 0 0</pose>
      <parent>pioneer3at_model::p3at/base_link</parent>
      <child>tether_model::tether/sphere_01</child>
      <axis>
        <xyz>0.000000 1.000000 0.000000</xyz>
        <limit>
          <lower>-3.000000</lower>
          <upper>3.000000</upper>
        </limit>
        <dynamics>
          <damping>1.000000</damping>
          <friction>0.000000</friction>
        </dynamics>
      </axis>
    </joint> 

    <joint name="marsupial/tether_ardrone_joint" type="revolute">
      <pose>0.01 0 -0.04 0 -1.57 0</pose>
      <parent>tether_model::tether/sphere_100</parent>
      <child>ardrone_model::ardrone/base_link</child>
      <axis>
        <xyz>0.000000 1.000000 0.000000</xyz>
        <limit>
          <lower>-3.00000</lower>
          <upper>3.00000</upper>
        </limit>
        <dynamics>
          <damping>1.000000</damping>
          <friction>0.000000</friction>
        </dynamics>
      </axis>
    </joint> 

    <!-- <plugin name='plugin_tf_gazebo_ros' filename='libplugin_tf_gazebo_ros.so'>
      <robotNamespace>marsupial</robotNamespace>
      <updateRate>50.0</updateRate>
    </plugin> -->

  </model>
</sdf>
