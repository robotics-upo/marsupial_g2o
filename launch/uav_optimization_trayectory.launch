<launch>

    <node pkg="marsupial_g2o" type="marsupial_node" name="marsupial_node" output="screen">
        <!-- <rosparam command="load" file="$(find marsupial_g2o)/cfg/uav_optimization_trayectory.yaml"/> -->
        <param name="X1" value="25.0"/>
        <param name="Y1" value="40.0"/>
        <param name="Z1" value="50.0"/>
        <param name="X2" value="50.0"/>
        <param name="Y2" value="15.0"/>
        <param name="Z2" value="40.0"/>
        <param name="w_alpha"   value="0.1"/>   <!-- distance-vertex -->
        <param name="w_beta"    value="0.12"/>   <!-- obstacles -->
        <param name="w_gamma"   value="0.1"/>   <!-- distance-xyz -->
        <param name="w_delta"   value="0.1"/>   <!-- kinematics -->
        <param name="w_epsilon" value="0.1"/>   <!-- equi-distance -->
        <param name="w_zeta"    value="0.1"/>   <!-- time -->
        <param name="w_eta"     value="0.1"/>   <!-- velocity -->
        <param name="w_theta"   value="0.001"/>   <!-- acceleration -->
    </node>

    <!-- <node pkg="rviz" type="rviz" name="rviz" args="-d $(find theta_star)/viz/mbzirc.rviz" output="screen"/>   -->
</launch>