<launch>

    <arg name="initial_pos_ugv_x"         default="-2.0"/>   
    <arg name="initial_pos_ugv_y"         default="-10.0"/>   
    <arg name="initial_pos_ugv_z"         default="0.0"/>
    <arg name="initial_pos_ugv_roll"      default="0.0"/>   
    <arg name="initial_pos_ugv_pitch"     default="0.0"/>   
    <arg name="initial_pos_ugv_yaw"       default="-1.5707"/>      

    <!-- <arg name="map_file" default="octomap_full"/> -->  # -3.8 -1.0 0 3.14 0 0
	<!-- <arg name="map_file" default="stage_1"/>  # 0 0 0 3.14 0 0  -->
	<!-- <arg name="map_file" default="stage_2"/> -->
	<!-- <arg name="map_file" default="stage_3"/> -->
	<!-- <arg name="map_file" default="stage_4"/> -->
	<!-- <arg name="map_file" default="stage_5"/> -->
	<arg name="map_file" default="rehersal_1_with_windows"/>
    

    <node pkg="tf" type="static_transform_publisher" name="map" 
        args="0 0 0 0 0 0  world /map 10"/> <!-- x y z y p r-->
    <node pkg="tf" type="static_transform_publisher" name="uav_base_link"   
        args="$(arg initial_pos_ugv_x) $(arg initial_pos_ugv_y) 3.0 $(arg initial_pos_ugv_yaw) $(arg initial_pos_ugv_pitch) $(arg initial_pos_ugv_roll)  /map /uav_base_link 10"/> <!-- x y z y p r-->
    <node pkg="tf" type="static_transform_publisher" name="ugv_base_link"   
        args="$(arg initial_pos_ugv_x) $(arg initial_pos_ugv_y) $(arg initial_pos_ugv_z) $(arg initial_pos_ugv_yaw) $(arg initial_pos_ugv_pitch) $(arg initial_pos_ugv_roll)  /map /ugv_base_link 10"/> <!-- x y z y p r-->
    <node pkg="tf" type="static_transform_publisher" name="reel_base_link"  
        args="0.5 0.0 0.2 -0.0 0.0 0.0  /ugv_base_link /reel_base_link 10"/> <!-- x y z y p r-->

    <include file="$(find marsupial_g2o)/launch/octomap_server.launch">
        <arg name="map_file"  value="$(arg map_file)"/>    
    </include>

    <!-- <include file="$(find marsupial_g2o)/launch/3d_global_and_optimizer_local_planner.launch"/> -->

    <!-- <node pkg="rviz" type="rviz" name="rviz" args="-d $(find marsupial_g2o)/viz/marsupial.rviz" output="screen"/>   -->
</launch>